{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "> 1%"
      },
      "modules": false,
      "useBuiltIns": true,
    }],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
